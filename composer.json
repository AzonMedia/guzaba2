{
    "name": "guzaba/guzaba2",
    "type": "library",
    "description": "PHP Framework for swoole",
    "keywords": ["framework","swoole"],
    "homepage": "https://github.com/AzonMedia/guzaba2",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vesko Kenashkov",
            "email": "kenashkov@azonmedia.com",
            "homepage": "http://azonmedia.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-pdo": "*",
        "ext-mysqli": "*",
        "ext-swoole": ">=4.4.8",
        "ext-json": "*",

        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/cache": "^1.0",
        "psr/log": "^1.1",

        "azonmedia/psr-to-swoole": "dev-master",
        "azonmedia/swoole-to-psr": "dev-master",
        "azonmedia/reflection": "dev-master",
        "azonmedia/registry": "dev-master",
        "azonmedia/routing": "dev-master",
        "azonmedia/url-rewriting": "dev-master",
        "azonmedia/debug": "dev-master",
        "azonmedia/di": "dev-master",
        "azonmedia/lock": "dev-master",
        "azonmedia/patterns": "dev-master",
        "azonmedia/transaction": "dev-master",
        "azonmedia/utilities": "dev-master",
        "azonmedia/apm": "dev-master",
        "azonmedia/watchdog": "dev-master",

        "jbzoo/utils": "^2.3",
        "ramsey/uuid": "^3.8",
        "symfony/cache": "^5.0"
    },
    "autoload": {
	    "files": ["src/Guzaba2/Kernel/Kernel.php"]
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.15",
        "friendsofphp/php-cs-fixer": "^2.15",
        "eaglewu/swoole-ide-helper": "dev-master"
    }
}
